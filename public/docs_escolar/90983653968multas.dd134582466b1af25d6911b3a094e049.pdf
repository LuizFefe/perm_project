		return true;
	}


	function IsNumeric(e) {
		var key;
		var keychar;
		var reg;

		if(window.event) {
		  // for IE, e.keyCode or window.event.keyCode can be used
		  key = e.keyCode;
		}
		else if(e.which) {
		  // netscape
		  key = e.which;
		}
		else {
		  // no event, so pass through
		  return true;
		}

		//keychar = String.fromCharCode(key);
		//reg = /\d/;
        //return reg.test(keychar);
  
  		if(window.event){   
			if (e.keyCode < 48 || e.keyCode > 57) {
				e.returnValue = false;
				e.cancel = true;
			}
		}
		else if (e.which){
			if (e.which < 48 || e.which > 57) {
				e.preventDefault();
				e.cancel = false;
			}
        }
		else if (e.charCode) {
			//NS 6+, Mozilla 0.9+
			if (e.charCode < 48 || e.charCode > 57) {
				e.preventDefault();
				e.cancel = false;
			}
		}
	}
</script>
